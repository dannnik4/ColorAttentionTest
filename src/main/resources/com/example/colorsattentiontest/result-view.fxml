<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.colorsattentiontest.ResultController">
    <padding>
        <Insets top="10" right="10" bottom="10" left="10"/>
    </padding>
    <spacing>10</spacing>
    <GridPane fx:id="colorGrid" hgap="10.0" vgap="10.0"/>
    <TableView fx:id="selectionTable" prefHeight="200.0" prefWidth="400.0">
        <columns>
            <TableColumn fx:id="selectedColorColumn" text="Selected Color"/>
            <TableColumn fx:id="correctColorColumn" text="Correct Color"/>
        </columns>
    </TableView>
    <Button fx:id="finishButton" text="Finish Attempt" onAction="#finishAttempt"/>
</VBox>
